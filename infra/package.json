{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "deploy": "npx cdk deploy --require-approval never",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap": "npx cdk bootstrap",
    "outputs": "powershell -NoProfile -Command \"aws cloudformation describe-stacks --stack-name InfraStack --query 'Stacks[0].Outputs' --output table\"",
    "cdn:invalidate": "powershell -NoProfile -Command ./invalidate.ps1 -Path '/*'"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.1029.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.214.0",
    "constructs": "^10.0.0"
  }
}
